{
	"manifest_version": 2,
	
	"name": "Quorator",
	"description": "Smart Quora Feed Display",
	"version": "1.0",
	
	"permissions": [
		"activeTab",
		"http://www.quora.com/api",
        "notifications"
	],
	
	"browser_action": {
		"default_icon": "Images/icon.png",
        "default_title": "Quorator",
		"default_popup": "popup.html"
	},
    
    "background": {
        "scripts": ["js/background.js"]
    }
}